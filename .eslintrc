{
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "plugins": ["import", "node", "promise", "standard", "jest"],
    "env": {
      "browser": true, 
      "node": true, 
      "jest/globals":true
    },
    "extends": [
      "eslint:recommended",
      "plugin:prettier/recommended"
    ],
    "rules": {
      "no-console": "error"
    }
  }